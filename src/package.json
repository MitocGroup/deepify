{
  "name": "@mitocgroup/deepify",
  "version": "0.1.27",
  "description": "Deep development tools",
  "author": {
    "name": "Mitoc Group",
    "email": "hello@mitocgroup.com",
    "website": "www.mitocgroup.com"
  },
  "contributors": [
    {
      "name": "DEEP Dev Team",
      "email": "hello@deep.mg",
      "website": "www.deep.mg"
    }
  ],
  "bugs": {
    "url": "https://github.com/MitocGroup/deepify/issues"
  },
  "keywords": [
    "DEEP",
    "Digital Enterprise End-To-End Platform",
    "Deep Core Services",
    "Package Manager"
  ],
  "bin": {
    "deepify": "./bin/deepify.js"
  },
  "os": [
    "!win32"
  ],
  "engines": {
    "node": ">=0.12",
    "npm": ">=2.10"
  },
  "main": "lib.compiled/bootstrap.js",
  "dependencies": {
    "@mitocgroup/deep-package-manager": "*",
    "@mitocgroup/deep-db": "*",
    "@mitocgroup/deep-core": "*",
    "joi": "6.4.*",
    "jsonfile": "1.1.*",
    "underscore.string": "2.3.*",
    "moment": "2.10.*",
    "aws-sdk": "2.1.*",
    "archiver": "0.14.*",
    "sync-exec": "0.6.*",
    "mkdirp": "0.5.*",
    "mime": "1.3.*",
    "crc": "3.3.*",
    "tar": "2.1.*",
    "fstream": "1.0.*",
    "fs-extra": "0.23.*",
    "tmp": "0.0.*",
    "proxyquire": "1.5.*",
    "v8-profiler": "5.2.*",
    "crypto": "0.0.*",
    "traceviewify": "0.4.*",
    "ignore": "2.2.*",
    "raml-validate": "1.0.*",
    "raml-sanitize": "1.0.*",
    "object-merge": "2.5.*",
    "prompt-sync": "3.0.*",
    "amazon-api-gateway-client": "0.1.*"
  },
  "devDependencies": {
    "chai": "3.2.*"
  },
  "scripts": {
    "coverage": "babel-node `which istanbul` cover --report lcovonly _mocha -- --ui tdd --recursive --reporter spec",
    "postinstall": "hooks/assure_deep_dev.js && npm run link-deep-package-manager && hooks/autocomplete_deepify.sh",
    "test": "mocha --ui tdd --compilers js:mocha-babel --recursive --reporter spec",
    "compile": "if [ -d 'lib/' ]; then BABEL_ENV=production babel lib/ --out-dir lib.compiled/; fi",
    "prepublish": "npm run compile",
    "link-deep-package-manager": "if [ -d 'lib/' ]; then ln -s `pwd`/node_modules/\\@mitocgroup/deep-package-manager/lib.compiled/* `pwd`/lib/ || true; fi"
  },
  "preferGlobal": true,
  "analyze": true
}
