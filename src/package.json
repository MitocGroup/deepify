{
  "name": "@mitocgroup/deepify",
  "version": "0.1.79",
  "description": "DEEP Development Tools",
  "keywords": [
    "Digital Enterprise End-To-End Platform",
    "Amazon Web Services",
    "Platform-as-a-Service",
    "DEEP",
    "AWS",
    "PaaS",
    "Cloud",
    "Computing",
    "Microservices",
    "Architecture",
    "Serverless",
    "Abstracted",
    "Development",
    "Tools"
  ],
  "homepage": "https://github.com/MitocGroup/deepify",
  "bugs": {
    "url": "https://github.com/MitocGroup/deepify/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Mitoc Group",
    "email": "hello@mitocgroup.com",
    "url": "http://www.mitocgroup.com"
  },
  "contributors": [
    {
      "name": "AlexanderC",
      "email": "alexanderc@mitocgroup.com"
    },
    {
      "name": "Marcel Goria",
      "email": "mgoria@mitocgroup.com"
    }
  ],
  "bin": {
    "deepify": "./bin/deepify.js"
  },
  "main": "lib.compiled/bootstrap.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/MitocGroup/deepify.git"
  },
  "scripts": {
    "coverage": "babel-node `which istanbul` cover --report lcovonly _mocha -- --ui tdd --recursive --reporter spec",
    "postinstall": "npm run link-deep-package-manager && npm run link-deep-package-manager-npm && npm run link-deep-package-manager-npm-subdirs && hooks/assure_deep_dev.js && hooks/autocomplete_deepify.sh",
    "test": "mocha --ui tdd --compilers js:mocha-babel --recursive --reporter spec",
    "compile": "if [ -d 'lib/' ]; then BABEL_ENV=production babel lib/ --out-dir lib.compiled/; fi",
    "prepublish": "npm run compile",
    "link-deep-package-manager": "ln -s `pwd`/node_modules/\\@mitocgroup/deep-package-manager/lib.compiled/* `pwd`/lib.compiled || true",
    "link-deep-package-manager-npm": "ln -s `pwd`/node_modules/\\@mitocgroup/deep-package-manager/node_modules/* `pwd`/node_modules || true",
    "link-deep-package-manager-npm-subdirs": "ln -s `pwd`/node_modules/\\@mitocgroup/deep-package-manager/node_modules/\\@mitocgroup/* `pwd`/node_modules/\\@mitocgroup || true"
  },
  "dependencies": {
    "@mitocgroup/deep-package-manager": "*",
    "proxyquire": "1.5.*",
    "v8-profiler": "5.2.*",
    "traceviewify": "0.4.*",
    "open": "0.0.*"
  },
  "devDependencies": {
    "chai": "3.2.*"
  },
  "engines": {
    "node": ">=0.12",
    "npm": ">=2.10"
  },
  "os": [
    "!win32"
  ],
  "preferGlobal": true,
  "analyze": true
}
