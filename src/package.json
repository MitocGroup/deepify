{
  "name": "deepify",
  "version": "1.0.19",
  "description": "DEEP Development Tools",
  "keywords": [
    "Digital Enterprise End-To-End Platform",
    "Amazon Web Services",
    "Platform-as-a-Service",
    "DEEP",
    "AWS",
    "PaaS",
    "Cloud",
    "Computing",
    "Microservices",
    "Architecture",
    "Serverless",
    "Abstracted",
    "Development",
    "Tools"
  ],
  "homepage": "https://github.com/MitocGroup/deepify",
  "bugs": {
    "url": "https://github.com/MitocGroup/deepify/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Mitoc Group",
    "email": "hello@mitocgroup.com",
    "url": "http://www.mitocgroup.com"
  },
  "contributors": [
    {
      "name": "AlexanderC",
      "email": "alexanderc@mitocgroup.com"
    },
    {
      "name": "Marcel Goria",
      "email": "mgoria@mitocgroup.com"
    }
  ],
  "bin": {
    "deepify": "./bin/deepify.js"
  },
  "main": "lib.compiled/bootstrap.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/MitocGroup/deepify.git"
  },
  "scripts": {
    "coverage": "babel-node `which istanbul` cover --report lcovonly _mocha -- --ui tdd --recursive --reporter spec",
    "preinstall": "npm run compile",
    "postinstall": "npm run link-deep-package-manager && npm run link-deep-package-manager-npm && hooks/assure_deep_dev.js && hooks/autocomplete_deepify.sh",
    "test": "mocha --ui tdd --compilers js:mocha-babel --recursive --reporter spec",
    "compile": "if [ -d 'lib/' ]; then BABEL_ENV=production babel lib/ --out-dir lib.compiled/; fi",
    "obfuscate-compiled": "if [ -d 'lib/' ]; then npm list -g --depth 1 uglify > /dev/null 2>&1 || npm install uglify -g; for f in $(find lib.compiled -type f -name *.js); do uglify -s ${f} -o ${f}; done; fi",
    "prepublish": "npm run obfuscate-compiled",
    "link-deep-package-manager": "ln -s `pwd`/node_modules/deep-package-manager/lib.compiled/* `pwd`/lib.compiled || true",
    "link-deep-package-manager-npm": "ln -s `pwd`/node_modules/deep-package-manager/node_modules/* `pwd`/node_modules || true"
  },
  "dependencies": {
    "deep-package-manager": "1.0.*",
    "proxyquire": "1.5.*",
    "v8-profiler": "5.2.*",
    "traceviewify": "0.4.*",
    "open": "0.0.*"
  },
  "devDependencies": {
    "aws-sdk": "2.1.*",
    "chai": "3.2.*",
    "sinon": "1.17.*",
    "sinon-chai": "2.8.*"
  },
  "engines": {
    "node": ">=0.12",
    "npm": ">=2.10"
  },
  "os": [
    "!win32"
  ],
  "preferGlobal": true,
  "analyze": true
}
